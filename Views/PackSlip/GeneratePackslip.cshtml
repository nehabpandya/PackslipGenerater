        </tr>

        <tr>
            <td colspan="2">GSTIN</td>
            <td colspan="3">@Model.GSTIN</td>
            <td class="no-bottom-border" colspan="5"></td>
        </tr>
        <tr>
            <td colspan="2">FEI#</td>

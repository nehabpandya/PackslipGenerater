        </tr>

        <tr>
            <td colspan="5">@Model.SNAddLine7</td>
            <td class="no-bottom-border" colspan="5">1 Medical Device</td>
        </tr>

        <tr>
            <td colspan="2">CIN</td>
            <td colspan="3">@Model.CIN</td>
